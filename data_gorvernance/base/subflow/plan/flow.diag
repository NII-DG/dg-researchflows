blockdiag {
    node_width = 200;
    node_height = 130;
    span_width = 20;
    span_height = 30;
    default_fontsize = 10;
    orientation = portrait;

    group {
        shape = line;
        style = none;

        group {
            color = "#FFFFCC";
            RF002001;
        }

        group {
            color = "#FFFFCC";
            RF009002[stacked];
        }
    }

    RF002001 -> RF009002
}
