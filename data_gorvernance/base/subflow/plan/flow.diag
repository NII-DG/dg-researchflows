blockdiag {
    node_width = 200;
    node_height = 130;
    span_width = 10;
    span_height = 30;
    default_fontsize = 10;
    orientation = portrait;

    group {
        shape = line;
        style = none;

        group RF002001 {
            color = "#FFFFCC";
            RF002001;
        }

        group RF009002 {
            color = "#FFFFCC";
            RF009002[stacked];
        }
    }

    RF002001 -> RF009002
}
